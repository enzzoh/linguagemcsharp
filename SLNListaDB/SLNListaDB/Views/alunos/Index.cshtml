@{
}
<div>
    <h1>Lista </h1>
</div>


<div id=" Divtabela">
    <table class="table table-striped">
        <thead>
            <tr>

                <th scope="col"> Codigo</th>
                <th scope="col"> Nome </th>
                <th scope="col"> Sobrenome </th>

            </tr>







        </thead>
        <tbody>

           @foreach(var item in ViewBag.Listalunos)
            {

            <tr>
                <td>@item.alunoID</td>
                <td>@item.NomeAluno</td>
                <td>@item.sobrenomeAluno</td>
            </tr>



            }
        </tbody>






    </table>

</div>