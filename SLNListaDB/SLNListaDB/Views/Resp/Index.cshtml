@{
}
<div>
    <h1>Lista RESPONSAVEIS</h1>
</div>


<div id=" Divtabela">
    <table class="table table-striped">
        <thead>
            <tr>

                <th scope="col"> Codigo</th>
                <th scope="col"> Nome </th>
                <th scope="col"> Sobrenome </th>

            </tr>







        </thead>
        <tbody>

            @foreach (var item in ViewBag.listaresponsaveis)
            {

                <tr>

                    <td>@item.respID</td>
                    <td>@item.NomeResp</td>
                    <td>@item.SobrenomeResp</td>















                </tr>






            }
        </tbody>






    </table>

</div>